{
  "slug": "maximum-score-of-a-split",
  "functionName": "maximumScoreOfASplit",
  "params": [
    {
      "name": "n",
      "type": "int"
    }
  ],
  "returnType": "int",
  "_status": "complete",
  "description": "Given a binary string s (passed via n encoding), split it into two non-empty parts and return the maximum score, where score equals the number of 0s in the left part plus the number of 1s in the right part.",
  "examples": [
    {"input": "s=\"011101\"", "output": "5"},
    {"input": "s=\"00111\"", "output": "5"}
  ],
  "constraints": ["2 <= s.length <= 500", "s consists of '0' and '1' only"],
  "testCases": [
    {
      "id": "tc-1",
      "inputs": {"n": 11},
      "expected": 5,
      "explanation": "s='011101': split at idx 1: '0'|'11101' → 1 zero + 4 ones = 5. This is the max over all splits.",
      "tags": ["basic"]
    },
    {
      "id": "tc-2",
      "inputs": {"n": 5},
      "expected": 5,
      "explanation": "s='00111': split at idx 2: '00'|'111' → 2 zeros + 3 ones = 5.",
      "tags": ["basic"]
    },
    {
      "id": "tc-3",
      "inputs": {"n": 15},
      "expected": 3,
      "explanation": "s='1111': split at idx 1: '1'|'111' → 0+3=3; split at 2: '11'|'11' → 0+2=2; max=3.",
      "tags": ["all-ones"]
    },
    {
      "id": "tc-4",
      "inputs": {"n": 0},
      "expected": 1,
      "explanation": "s='00': split at idx 1: '0'|'0' → 1+0=1. Only valid split; score=1.",
      "tags": ["all-zeros"]
    },
    {
      "id": "tc-5",
      "inputs": {"n": 2},
      "expected": 2,
      "explanation": "s='010': split at idx 1: '0'|'10' → 1+1=2; split at idx 2: '01'|'0' → 1+0=1. Max=2.",
      "tags": ["basic"]
    }
  ]
}
