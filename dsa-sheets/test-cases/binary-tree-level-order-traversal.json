  "testCases": [
    {
      "id": "tc-1",
      "inputs": { "root": [3, 9, 20, null, null, 15, 7] },
      "expected": [[3], [9, 20], [15, 7]],
      "tags": ["basic"]
    },
    {
      "id": "tc-2",
      "inputs": { "root": [1] },
      "expected": [[1]],
      "tags": ["single-element"]
    },
    {
      "id": "tc-3",
      "inputs": { "root": [] },
      "expected": [],
      "tags": ["empty-input", "edge-case"]
    },
    {
      "id": "tc-4",
      "inputs": { "root": [1, 2, 3, 4, 5, 6, 7] },
      "expected": [[1], [2, 3], [4, 5, 6, 7]],
