    },
    {
      "id": "tc-2",
      "inputs": { "root": [2, 1, 3] },
      "expected": [2, 3, 1],
      "tags": ["basic"]
    },
    {
      "id": "tc-3",
      "inputs": { "root": [] },
      "expected": [],
