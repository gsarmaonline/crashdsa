{
  "name": "crashdsa",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run --hot index.ts",
    "start": "bun run index.ts",
    "test": "bun test",
    "api-docs": "bun run scripts/generate-api-docs.ts",
    "api-docs:serve": "bun run dev",
    "screenshots": "bun run scripts/take-screenshots.ts",
    "dsa-sheets:fetch": "bun run scripts/dsa-sheets/fetch-sheets.ts",
    "dsa-sheets:process": "bun run scripts/dsa-sheets/process-sheets.ts",
